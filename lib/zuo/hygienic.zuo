#lang zuo/kernel

(let ([macromod (module->hash 'zuo/private/macromod)])
  (let ([main (module->hash 'zuo/private/macromod/main)])
    (hash
     'read-and-eval ((hash-ref macromod 'make-read-and-eval-with-initial-imports-from #f)
                     'zuo/private/macromod/main)
     'macromod-provides (hash-ref main 'macromod-provides #f))))
