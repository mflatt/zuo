#lang zuo/datum

(require "and-or.zuo"
         "../pair.zuo"
         "define.zuo"
         "syntax-error.zuo")

(provide char)

(define-syntax (char stx)
  (if (and (= 2 (length stx))
           (string? (cadr stx))
           (= 1 (string-length (cadr stx))))
      (string-ref (cadr stx) 0)
      (bad-syntax stx)))


